module MyLib (someFunc) where

someFunc :: IO ()
someFunc = putStrLn "someFunc"

{- primitive types -}

{- declaring integer variables -}
x, y, z :: Int
x = 5
y = 4
z = 6



{- declaring double-precision fp -}
d1, d2 :: Double
d1 = 4.5
d2 = 8.888

sumDouble :: Double -> Double
sumDouble x = x + d1 + d2

{- declaring booleans -}
b1, b2 :: Bool
b1 = True
b2 = False








